才发现洛谷有P1000这题，“万事开头难”，溜了溜了

#include<iostream>
using namespace std;
int main(){
    cout << "                ********" << endl;
	cout << "               ************" << endl;
	cout << "               ####....#." << endl;
	cout << "             #..###.....##...." << endl;
	cout << "             ###.......######              ###            ###" << endl;
	cout << "                ...........               #...#          #...#" << endl;
	cout << "               ##*#######                 #.#.#          #.#.#" << endl;
	cout << "            ####*******######             #.#.#          #.#.#" << endl;
	cout << "           ...#***.****.*###....          #...#          #...#" << endl;
	cout << "           ....**********##.....           ###            ###" << endl;
	cout << "           ....****    *****...." << endl;
	cout << "             ####        ####" << endl;
	cout << "           ######        ######" << endl;
	cout << "##############################################################" << endl;
	cout << "#...#......#.##...#......#.##...#......#.##------------------#" << endl;
	cout << "###########################################------------------#" << endl;
	cout << "#..#....#....##..#....#....##..#....#....#####################" << endl;
	cout << "##########################################    #----------#" << endl;
	cout << "#.....#......##.....#......##.....#......#    #----------#" << endl;
	cout << "##########################################    #----------#" << endl;
	cout << "#.#..#....#..##.#..#....#..##.#..#....#..#    #----------#" << endl;
	cout << "##########################################    ############" << endl;
	return 0;
}
